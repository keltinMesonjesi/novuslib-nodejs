{
  "name": "novuslib-nodejs",
  "version": "1.0.0",
  "description": "API built with nodejs for novuslib library management system",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js",
    "test": "jest -i --colors --verbose --detectOpenHandles",
    "coverage": "jest -i --coverage",
    "coverage:coveralls": "jest -i --coverage --coverageReporters=text-lcov | coveralls",
    "make:migration": "npx sequelize-cli migration:generate",
    "make:seed": "npx sequelize-cli seed:generate",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "seed:all": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo",
    "seed:undo:all": "npx sequelize-cli db:seed:undo:all",
    "make:model": "npx sequelize-cli model:generate",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keltinMesonjesi/novuslib-nodejs.git"
  },
  "author": "Keltin Mesonjesi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/keltinMesonjesi/novuslib-nodejs/issues"
  },
  "homepage": "https://github.com/keltinMesonjesi/novuslib-nodejs#readme",
  "dependencies": {
    "@paralleldrive/cuid2": "^2.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "http-status": "^1.5.3",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.8.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pug": "^3.0.2",
    "sequelize": "^6.28.0",
    "sequelize-cli": "^6.5.2",
    "swagger-ui-express": "^4.6.0",
    "winston": "^3.8.2",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.30.0",
    "jest": "^29.4.2",
    "node-mocks-http": "^1.12.1",
    "prettier": "2.8.1",
    "supertest": "^6.3.3"
  }
}
